{
  "name": "@originvault/ov-types",
  "version": "1.0.0",
  "description": "Type-safe schemas for OriginVault multi-root trust architecture",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && npm run generate-types",
    "generate-types": "node dist/exportTypes.js",
    "dev": "tsc --watch",
    "server": "node dist/server.js",
    "dev:server": "tsc && npm run server",
    "start": "npm run build && npm run server",
    "prepare": "npm run build",
    "type-check": "tsc --noEmit",
    "pre-push": "npm run type-check && npm run build"
  },
  "dependencies": {
    "@openverifiable/open-verifiable-types": "^0.0.1",
    "json-schema-to-typescript": "^13.1.1",
    "quicktype-core": "^23.2.6",
    "react-social-icons": "^6.24.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "body-parser": "^1.20.2",
    "jszip": "^3.10.1",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "react-dropzone": "^14.2.3"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/express": "^4.17.17",
    "@types/cors": "^2.8.13",
    "@types/jszip": "^3.4.1",
    "typescript": "^5.0.0"
  },
  "files": [
    "dist/",
    "types/",
    "schemas/",
    "contexts/",
    "examples/"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/originvault/originvault-schema-registry"
  },
  "keywords": [
    "originvault",
    "verifiable-credentials",
    "did",
    "trust-registry",
    "multi-root",
    "schemas"
  ]
}
